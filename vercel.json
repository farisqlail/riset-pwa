{
    "version": 2,
    "builds": [
        {
            "src": "nuxt.config.js",
            "use": "@nuxtjs/vercel-builder"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/sw.js",
            "dest": "/_nuxt/static/sw.js"
        }
    ],
    "cleanUrls": true,
    "optimize": {
        "include": [
            "favicon.ico",
            "img/**/*",
            "fonts/**/*"
        ]
    },
    "env": {
        "NUXT_ENV_API_URL": "https://riset-pwa-fawn.vercel.app/"
    }
}