{
    "buildCommand": "yarn run",
    "version": 2,
    "builds": [
        {
            "src": "nuxt.config.js",
            "use": "@nuxtjs/vercel-builder",
            "config": {
                "serverFiles": [
                    "package.json"
                ]
            }
        },
        {
          "src": "pages/index.vue",
          "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/sw.js",
            "continue": true,
            "headers": {
                "Cache-Control": "public, max-age=0, must-revalidate",
                "Service-Worker-Allowed": "/"
            },
        },
        {
          "src": "/(.*)",
         "dest": "pages/index.vue"
        }
    ]
}
